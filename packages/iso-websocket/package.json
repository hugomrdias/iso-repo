{
  "name": "iso-websocket",
  "type": "module",
  "version": "0.3.0",
  "description": "Isomorphic, resilient and extensible Websocket.",
  "author": "Hugo Dias <hugomrdias@gmail.com> (hugodias.me)",
  "license": "MIT",
  "homepage": "https://github.com/hugomrdias/iso-repo/tree/master/packages/iso-websocket",
  "repository": {
    "url": "hugomrdias/iso-repo",
    "directory": "packages/iso-websocket"
  },
  "bugs": {
    "url": "https://github.com/hugomrdias/iso-repo/issues"
  },
  "keywords": [
    "websocket",
    "reconnect",
    "retry",
    "websocket-client"
  ],
  "exports": {
    ".": {
      "types": "./dist/src/index.d.ts",
      "default": "./src/index.js"
    }
  },
  "main": "src/index.js",
  "types": "dist/src/index.d.ts",
  "files": [
    "src",
    "dist/src/*.d.ts",
    "dist/src/*.d.ts.map"
  ],
  "scripts": {
    "lint": "tsc --build && biome check --no-errors-on-unmatched --files-ignore-unknown=true .",
    "test": "tsc --build && pnpm run test:node && pnpm run test:browser",
    "test:node": "playwright-test 'test/**/!(*.browser).test.js' --mode node",
    "test:browser": "playwright-test 'test/**/!(*.node).test.js' --assets test/mocks"
  },
  "dependencies": {
    "debug": "^4.3.7",
    "retry": "^0.13.1",
    "typescript-event-target": "^1.1.0",
    "unws": "^0.2.4",
    "ws": "^8.18.0"
  },
  "devDependencies": {
    "@types/debug": "^4.1.12",
    "@types/node": "^22.5.5",
    "@types/retry": "^0.12.5",
    "@types/ws": "^8.5.12",
    "delay": "^6.0.0",
    "p-defer": "^4.0.0",
    "playwright-test": "^14.1.6",
    "type-fest": "^4.26.1",
    "typescript": "5.6.2"
  },
  "publishConfig": {
    "provenance": true
  },
  "depcheck": {
    "specials": [
      "bin"
    ],
    "ignores": [
      "@types/*",
      "assert",
      "type-fest"
    ]
  }
}
